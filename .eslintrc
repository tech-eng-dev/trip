{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jest": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": 0,
    "no-prototype-builtins": 0,
    "no-console": 0,
    "no-alert": 0,
    "class-methods-use-this": 0,
    "react/prop-types": 0,
    "react/forbid-prop-types": 0,
    "no-nested-ternary": 0,
    "import/no-named-as-default": ["off"],
    "react/jsx-no-bind": 0,
    "react/no-did-mount-set-state": 0,
    "react/sort-comp": ["off"],
    "react/prefer-stateless-function": ["off"],
    "no-useless-escape": 0,
    "arrow-parens": 0
  },
  "globals": {
    "testHelper": true,
    "memoryDB": true
  }
}
